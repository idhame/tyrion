<div id="wrapper">
        <!-- Navigation -->
        <%= render 'pages/nav' %>
        <!-- Page Content -->
        <div id="page-wrapper">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-lg-12">
                        <h1 class="page-header">Manage Users</h1>
                        <% @users.each do |user| %>
<% if current_user != user %>
<%= user.email %>
<%= link_to 'Delete User', user, data: { method: :delete, confirm: 'Are you sure?' } %>
<% end %>
<br/>
<% end %>
                    </div>
                    <!-- /.col-lg-12 -->
                </div>
                <!-- /.row -->
            </div>
            <!-- /.container-fluid -->
        </div>
        <!-- /#page-wrapper -->
</div>

